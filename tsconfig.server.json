{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["es2017", "dom"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "module": "commonjs",
    "noEmit": false,
    "outDir": "./dist_server"
  },
  "files": [
    "\/app\/src\/test-utils\/component-test-server\/server_for_compile.ts",
    "\/app\/src\/components\/__tests__\/ControlsTestHarness.tsx",
    "\/app\/src\/components\/Controls.tsx",
    "\/app\/src\/services\/fft-status.ts",
    "\/app\/src\/types\/index.ts"
  ]
}
