{
  "name": "react-app",
  "version": "2.1.2",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^20.17.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "test:ct:selenium:server": "node ./dist_server/test-utils/component-test-server/server_for_compile.js",
    "test:ct:selenium": "jest --config jest.config.selenium.js --testMatch \"**/src/components/__tests__/**/*.selenium.test.ts\"",
    "test:e2e:selenium": "jest --config jest.config.selenium.js --testMatch \"**/e2e/**/*.selenium.test.ts\"",
    "test:selenium:all": "jest --config jest.config.selenium.js",
    "start": "REACT_APP_BUILD_VERSION=$(date +\"%Y%m%d-%H%M\") nx serve",
    "build": "REACT_APP_BUILD_VERSION=$(date +\"%Y%m%d-%H%M\") nx build",
    "test": "nx test",
    "dev": "REACT_APP_BUILD_VERSION=$(date +\"%y%m%d-%H%M\") nx serve"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@nx/vite": "21.1.3",
    "@nx/web": "21.1.3",
    "@rollup/plugin-replace": "^6.0.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/express": "^4.17.13",
    "@types/ignore-styles": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/ui": "^3.0.0",
    "eslint-config-react-app": "^7.0.1",
    "express": "^5.1.0",
    "geckodriver": "^4.4.1",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "jiti": "2.4.2",
    "nx": "21.1.3",
    "selenium-webdriver": "^4.33.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "vite": "^6.3.5",
    "vitest": "^3.2.3",
    "web-vitals": "^5.0.2"
  }
}
